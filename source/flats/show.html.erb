<!-- Access data.flats with owner's name as key -->
<% flat = data.flats[owner] %>

<!-- Inject flat data in template -->
<div class="main-container">
  <%= image_tag "https://kitt.lewagon.com/placeholder/cities/#{flat.city_slug}", class:"profile-banner" %>
  <%= image_tag "https://kitt.lewagon.com/placeholder/users/#{owner}", class:"profile-image" %>
</div>

<p><%= flat.name %></p>
<h1><%= flat.description %></h1>
<p><%= flat.city_name %></p>
